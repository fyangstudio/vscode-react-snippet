  "React Router Link": {
    "prefix": ["rl", "link"],
    "body": "<Link to={${1:path}}>${2:anchorText}</Link>\n"
  },
  "React Router NavLink": {
    "prefix": ["rnl", "navlink"],
    "body": "<NavLink to={${1:path}}>${2:anchorText}</NavLink>\n"
